#!/usr/bin/env python

import rc.utils as rcu


stack = rcu.stack()

print()
for key in sorted(stack['sys'].keys()):
    print('  %-20s : %s' % (key, stack['sys'][key]))
print()
for key in sorted(stack['rc'].keys()):
    print('  %-20s : %s' % (key, stack['rc'][key]))
print()
for key in sorted(stack['radical'].keys()):
    print('  %-20s : %s' % (key, stack['radical'][key]))
print()


